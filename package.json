    {
      "type": "GitHub - donavanbecker",
      "url": "https://github.com/sponsors/donavanbecker"
    },
    {
      "type": "Paypal - Mantorok1",
      "url": "https://paypal.me/Mantorok1"
    }
  ],
  "keywords": [
    "homebridge-plugin",
    "irrigation",
    "valve",
    "rainbird"
  ],
  "dependencies": {
    "@homebridge/plugin-ui-utils": "^0.0.19",
    "aes-js": "^3.1.2",
    "axios": "1.3.2",
    "concurrent-queue": "^7.0.2",
    "queue": "^6.0.2",
    "rxjs": "^7.6.0",
    "super-stringify": "^1.0.0",
