    "@typescript-eslint/parser": "^5.46.0",
    "eslint": "^8.29.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-prettier": "4.2.1",
    "homebridge": "^1.6.0",
    "nodemon": "^2.0.20",
    "npm-check-updates": "^16.5.1",
    "prettier": "2.8.1",
    "rimraf": "^3.0.2",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4",
    "typescript-axios-wb": "^1.0.3"
  }
