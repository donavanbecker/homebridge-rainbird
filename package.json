    "nodemon": "^3.0.1",
    "npm-check-updates": "^16.14.11",
    "rimraf": "^5.0.5",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.2",
    "typescript-axios-wb": "^1.0.3"
